<div class="container mt-3">
  <div class="row justify-content-center">
    <div class="col-12">
      <h2>Verifi photo</h2>
      <div class="mt-3">
        <%= simple_form_for @result do |f| %>
          <%= f.input :photo, label: false, as: :file, input_html: {class: "bg-white"} %>
          <%= f.submit class: "button" %>
        <% end %>
      </div>
    </div>
  </div>
</div>
