<div class="container">
  <h1>All Posts</h1>
  <%= link_to "New Post", new_post_path if policy(Post).new? %>
  <% @posts.each do |post| %>
    <div class="cards">
      <%= post.title %>
      <%= post.user.user_name %>
      <%= post.content %>
      <%= post.photo %>
    </div>
  <% end %>
</div>
